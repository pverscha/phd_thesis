  langid = {english},
  keywords = {Bone cancer,Predictive markers,Proteome,Proteomic analysis},
  file = {/Users/pverscha/Zotero/storage/TTJCRHM4/Madda et al. - 2020 - Proteomic profiling and identification of signific.pdf}
