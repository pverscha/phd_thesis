  author = {Lohmann, Patrick and Sch{\"a}pe, Stephanie Serena and Haange, Sven-Bastiaan and Oliphant, Kaitlyn and {Allen-Vercoe}, Emma and Jehmlich, Nico and Von Bergen, Martin},
  year = {2020},
