@article{hassaMetagenomeMetatranscriptomeMetaproteome2018,
  title = {Metagenome, Metatranscriptome, and Metaproteome Approaches Unraveled Compositions and Functional Relationships of Microbial Communities Residing in Biogas Plants},
  author = {Hassa, Julia and Maus, Irena and Off, Sandra and P{\"u}hler, Alfred and Scherer, Paul and Klocke, Michael and Schl{\"u}ter, Andreas},
